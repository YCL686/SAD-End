<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sharablead.mapper.WatchMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.sharablead.entity.Watch">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="watched_id" property="watchedId" />
        <result column="watch_date" property="watchDate" />
        <result column="watched_type" property="watchedType"/>
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, watched_id, watched_type, watch_date, gmt_created, gmt_modified
    </sql>

</mapper>
