<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sharablead.mapper.AdAuctionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.sharablead.entity.AdAuction">
        <id column="id" property="id" />
        <result column="ad_id" property="adId" />
        <result column="bid_price" property="bidPrice" />
        <result column="deal_price" property="dealPrice" />
        <result column="deal_days" property="dealDays"/>
        <result column="total_price" property="totalPrice" />
        <result column="status" property="status" />
        <result column="auction_date" property="auctionDate" />
        <result column="auction_end_time" property="auctionEndTime" />
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ad_id, bid_price, deal_price, deal_days, total_price, status, auction_date, auction_end_time, gmt_created, gmt_modified
    </sql>

</mapper>
