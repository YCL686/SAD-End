<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sharablead.mapper.WithdrawRequestMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.sharablead.entity.WithdrawRequestRecord">
        <id column="id" property="id" />
        <result column="account_id" property="accountId" />
        <result column="withdraw_amount" property="withdrawAmount" />
        <result column="withdraw_address" property="withdrawAddress" />
        <result column="status" property="status" />
        <result column="memo" property="memo" />
        <result column="entry_id" property="entryId" />
        <result column="withdraw_date" property="withdrawDate" />
        <result column="tx_hash" property="txHash" />
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account_id, withdraw_amount, withdraw_address, status, memo, entry_id, withdraw_date, tx_hash, gmt_created, gmt_modified
    </sql>

</mapper>
