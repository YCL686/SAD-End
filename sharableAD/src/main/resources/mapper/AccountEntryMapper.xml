<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sharablead.mapper.AccountEntryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.sharablead.entity.AccountEntry">
        <id column="id" property="id" />
        <result column="account_id" property="accountId" />
        <result column="entry_type" property="entryType" />
        <result column="entry_event" property="entryEvent" />
        <result column="entry_amount" property="entryAmount" />
        <result column="entry_balance" property="entryBalance" />
        <result column="status" property="status" />
        <result column="tx_hash" property="txHash" />
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account_id, entry_type, entry_event, enrty_amount, entry_balance, tx_hash, gmt_created, gmt_modified
    </sql>

</mapper>
