<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sharablead.mapper.CronJobMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.sharablead.entity.CronJob">
        <id column="id" property="id" />
        <result column="cron_job_name" property="cronJobName" />
        <result column="cron_job_description" property="cronJobDescription" />
        <result column="cron_job_code" property="cronJobCode" />
        <result column="cron" property="cron" />
        <result column="memo" property="memo"/>
        <result column="status" property="status" />
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, cron_job_name, cron_job_description, cron_job_code, cron, memo, status, gmt_created, gmt_modified
    </sql>

</mapper>
